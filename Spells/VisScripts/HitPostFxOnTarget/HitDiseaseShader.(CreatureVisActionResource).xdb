<?xml version="1.0" encoding="UTF-8" ?>
<CreatureVisActionResource>
	<action type="VisActionList">
		<play>Simultaneously</play>
		<elements>
			<Item type="VisActionList">
				<play>Simultaneously</play>
				<playWhile type="VisActionDelay">
					<time>300</time>
				</playWhile>
				<elements>
					<Item type="ProceduralEffectVisAction">
						<timeOff>0.5</timeOff>
						<timeOn>0.1</timeOn>
						<proceduralEffect href="/Spells/FX/InterfaceFX/Disease.(ProceduralEffect).xdb#xpointer(/ProceduralEffect)" />
					</Item>
				</elements>
			</Item>
			<Item type="CreatureEffectsAction">
				<visualEffects>
					<Item>
						<locator>Slot_FX</locator>
						<effectFx href="/Spells/FX/Spells/HitDisease.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
						<fixPoint>true</fixPoint>
					</Item>
				</visualEffects>
			</Item>
		</elements>
	</action>
</CreatureVisActionResource>
