<?xml version="1.0" encoding="UTF-8" ?>
<SpellVisScripts>
	<launch type="VisActionList">
		<play>InSequence</play>
		<elements>
			<Item type="CreatureAnimationAction">
				<animations>
					<Item>drink</Item>
				</animations>
			</Item>
			<Item type="VisActionList">
				<play>Simultaneously</play>
				<elements>
					<Item type="CreatureEffectsAction">
						<visualEffects>
							<Item>
								<effectFx href="/Spells/FX/Spells/Dragon_Wings_Column.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
								<fixPoint>true</fixPoint>
								<member>Group</member>
							</Item>
						</visualEffects>
					</Item>
					<Item type="VisActionList">
						<elements>
							<Item type="VisActionDelay">
								<time>200</time>
							</Item>
							<Item type="VisActionList">
								<play>Simultaneously</play>
								<elements>
									<Item type="CreatureAnimationAction">
										<animations>
											<Item>levelUp</Item>
										</animations>
										<advanced>
											<speed>0.9</speed>
										</advanced>
									</Item>
									<Item type="VisActionList">
										<play>Simultaneously</play>
										<elements>
											<Item type="CreatureEffectsAction">
												<visualEffects>
													<Item>
														<effectFx href="/Spells/FX/Spells/Dragon_Wings_Wear.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
														<locator>Slot_TopFX</locator>
														<member>Group</member>
													</Item>
												</visualEffects>
											</Item>
											<Item type="VisActionList">
												<play>Simultaneously</play>
												<playWhile type="VisActionDelay">
													<time>3000</time>
												</playWhile>
												<elements>
													<Item type="CreatureEffectsAction">
														<visualEffects>
															<Item>
																<effectFx href="/Spells/FX/Spells/Dragon_Wings_Part.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
																<locator>Slot_TopFX</locator>
																<member>Group</member>
															</Item>
														</visualEffects>
													</Item>
												</elements>
											</Item>
										</elements>
									</Item>
								</elements>
							</Item>
						</elements>
					</Item>
				</elements>
			</Item>
		</elements>
	</launch>
</SpellVisScripts>
