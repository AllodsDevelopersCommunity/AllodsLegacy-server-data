<?xml version="1.0" encoding="UTF-8" ?>
<CreatureVisActionResource>
	<action type="VisActionList">
		<play>Simultaneously</play>
		<elements>
			<Item type="CreatureOnParameterChangedAction">
				<parameter type="GuildLevelParameter" />
				<parameterUpScript type="VisActionList">
					<visActionID />
					<play>Simultaneously</play>
					<playWhile />
					<elements>
						<Item type="CreatureAnimationAction">
							<visActionID />
							<mode>DIE</mode>
							<use>USE_ANIMATIONS</use>
							<animations>
								<Item>battleRoar</Item>
							</animations>
							<channel>CHANNEL_ABILITY</channel>
							<channelOverrideMode>OVERRIDE_DEFAULT</channelOverrideMode>
							<channelOverrideScript />
							<channelOverrideControllers>DEFAULT_CONTROLLERS</channelOverrideControllers>
							<specialController />
							<advanced>
								<speed>0</speed>
								<backgroundAfterFirstKey>false</backgroundAfterFirstKey>
								<changeChannelAfterFirstKey>NOT_CHANGE</changeChannelAfterFirstKey>
								<channelAfterFirstKey>CHANNEL_RETURNING</channelAfterFirstKey>
								<offendType>NO_OFFEND</offendType>
								<autoHasten>
									<mode>AUTOHASTEN_NONE</mode>
									<timeToBeLeft>0</timeToBeLeft>
								</autoHasten>
								<eventPlaybackEnd>false</eventPlaybackEnd>
								<turnToTarget>TURN_DEFAULT</turnToTarget>
								<waitRealEvent>WAIT_DEFAULT</waitRealEvent>
							</advanced>
						</Item>
						<Item type="CreatureEffectsAction">
							<visActionID />
							<visualEffects>
								<Item>
									<delay>0</delay>
									<effectFx href="/Spells/FX/Spells/LevelUp_Guild.(VisObjectTemplate).xdb#xpointer(/VisObjectTemplate)" />
									<effectAlias href="" />
									<effectAnimations>
										<mode>DIE</mode>
									</effectAnimations>
									<fixPoint>false</fixPoint>
									<lockScale>false</lockScale>
									<scale>1</scale>
									<fadeInTime>0</fadeInTime>
									<fadeOutTime>0</fadeOutTime>
									<locator>Slot_Global</locator>
									<member>Default_Member</member>
									<locatorName />
									<offendTime />
								</Item>
							</visualEffects>
							<stopOnDeath>false</stopOnDeath>
						</Item>
					</elements>
					<stopOnDeath>false</stopOnDeath>
				</parameterUpScript>
			</Item>
			<Item type="CreatureIfMainAvatarRunAction">
				<action type="VisActionList">
					<play>Simultaneously</play>
					<elements>
						<Item type="CreatureMusicParamAction">
							<parameter href="Action.(SoundParameter).xdb#xpointer(/SoundParameter)" />
							<creatureParameter type="ThreatParameter" />
						</Item>
					</elements>
				</action>
			</Item>
		</elements>
	</action>
</CreatureVisActionResource>
