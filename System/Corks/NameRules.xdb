<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<basement.server.lang.NameRules>
	<Header>
		<resourceId>122637348</resourceId>
	</Header>
	<name>eng</name>
	<lang>en</lang>
	<country>US</country>
	<avatarNameRules>
		<matchUser>[A-Za-z0-9_ \-]+</matchUser>
		<matchMaster>[A-Za-z0-9_ \-]+</matchMaster>
		<minLength>1</minLength>
		<maxLength>14</maxLength>
		<transformation>NoTransformation</transformation>
	</avatarNameRules>
  <shipNameRules>
		<matchUser>[A-Za-z]+(\-\d+)?</matchUser>
		<minLength>3</minLength>
		<maxLength>14</maxLength>
		<transformation>FirstUppercased</transformation>
	</shipNameRules>
  <guildNameRules>
		<matchUser>[A-Za-z]+( |\-)?[A-Za-z]+</matchUser>
		<minLength>3</minLength>
		<maxLength>14</maxLength>
		<transformation>FirstUppercased</transformation>
	</guildNameRules>
	<deniedWords href="deniedWords.txt" />
</basement.server.lang.NameRules>
