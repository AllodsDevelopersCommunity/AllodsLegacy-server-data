					</Item>
				</visualEffects>
			</Item>
		</elements>
	</wholeCasting>
	<launch type="VisActionList">
		<elements>
			<Item skiped="" />
			<Item type="VisActionList">
				<elements>
					<Item skiped="" />
					<Item type="VisActionList">
						<elements>
							<Item skiped="" />
							<Item type="CreatureProjectileAction">
								<projectileFx hrefvar="FXProjectile" />
								<explosionFx hrefvar="FXProjectileHit" />
