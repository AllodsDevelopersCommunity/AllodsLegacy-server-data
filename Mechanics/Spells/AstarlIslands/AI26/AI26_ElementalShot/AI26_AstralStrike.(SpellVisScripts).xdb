<?xml version="1.0" encoding="UTF-8" ?>
<SpellVisScripts>
	<Header>
		<Prototype href="/Spells/VisScripts/Prototypes/Projectile.(SpellVisScripts).xdb#xpointer(/SpellVisScripts)" />
