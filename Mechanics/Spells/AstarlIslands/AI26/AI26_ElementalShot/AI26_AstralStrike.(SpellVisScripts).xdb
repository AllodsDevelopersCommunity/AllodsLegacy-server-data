					</Item>
					<Item type="VisActionList">
						<elements>
							<Item type="VisActionDelay">
								<time>500</time>
							</Item>
							<Item skiped="" />
							<Item type="CreatureProjectileAction">
								<offendTime type="EffectOffendConstDelay">
									<delay>0</delay>
								</offendTime>
							</Item>
						</elements>
						<stopOnDeath>false</stopOnDeath>
					</Item>
				</elements>
			</Item>
		</elements>
	</launch>
</SpellVisScripts>
