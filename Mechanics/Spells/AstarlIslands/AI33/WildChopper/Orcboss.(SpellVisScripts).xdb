<?xml version="1.0" encoding="UTF-8" ?>
<SpellVisScripts>
	<Header>
		<Variables>
			<PreCastAnimation>/Spells/VisScripts/Prototypes/PreCastFXDisease.(CreatureVisActionResource).xdb#xpointer(/CreatureVisActionResource)</PreCastAnimation>
			<LocatorNameOnTargets />
			<FXOnTargets />
