											<Item type="VisActionStopAction">
												<visActionID />
												<stoppedActionID>EnchantAction2</stoppedActionID>
											</Item>
										</elements>
									</action>
								</Item>
								<Item>
									<action type="VisActionList">
										<play>Simultaneously</play>
										<elements>
											<Item type="CreatureTrailAction">
												<visActionID>TrailAction3</visActionID>
